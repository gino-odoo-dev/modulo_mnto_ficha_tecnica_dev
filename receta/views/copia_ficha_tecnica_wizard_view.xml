<odoo>
    <record id="view_copia_ficha_tecnica_wizard_form" model="ir.ui.view">
        <field name="name">copia.ficha.tecnica.wizard.form</field>
        <field name="model">copia.ficha.tecnica.wizard</field>
        <field name="arch" type="xml">
            <form string="Ficha Tecnica">
                <div class="row">
                    <div class="col-lg-6 col-md-6">
                        <group string="Materiales Base">
                            <field name="name" readonly="1"/>
                            <field name="name" readonly="1"/>
                            <field name="name" readonly="1"/>
                        </group>
                    </div>
                    <div class="col-lg-6 col-md-6">
                        <group string="Materiales Por Numero">
                            <field name="name" readonly="1"/>
                            <field name="name" readonly="1"/>
                            <field name="name" readonly="1"/>
                            <field name="name" readonly="1"/>
                            <field name="name" readonly="1"/>
                        </group>
                    </div>
                </div>
                <footer>
                    <button string="Cerrar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
